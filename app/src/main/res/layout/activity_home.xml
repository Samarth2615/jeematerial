<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".home"
    android:orientation="vertical">
    <androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/drawer_layout">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/title_bar">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbar"
                android:minHeight="?attr/actionBarSize"
                android:layout_width="match_parent"
                android:layout_height="60dp"
                app:titleTextColor="@android:color/white"
                android:background="?attr/colorPrimary"
                app:navigationIcon="@drawable/ic_side_nav">
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:text="@string/app_name"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/the_wanters_demo"
                    android:autoSizeMinTextSize="15dp"
                    android:autoSizeMaxTextSize="22dp"
                    android:autoSizeTextType="uniform"
                    android:textColor="@color/white"/>
            </androidx.appcompat.widget.Toolbar>
        <ScrollView
            android:id="@+id/scroll_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/toolbar"
            android:background="@color/colorAccent">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="10dp">
                <LinearLayout
                    android:id="@+id/ll1"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_marginBottom="10dp">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/jee_main_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/jee"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/jeemain"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/jeemain_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/jee"
                                android:text="Jee Main"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/jee_advanced_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/advanced"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/jeeadvanced"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/advanced_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/advanced"
                                android:text="Jee Advanced"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll2_1"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/ll1">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/civil_pre_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/civil_pre"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/civil_pre"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/civil_pre_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/civil_pre"
                                android:text="Civil Services (Preliminary)"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/civil_main_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/civil_main"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/civil_main"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/civil_main_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/civil_main"
                                android:text="Civil Services (Main)"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll2"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@+id/ll2_1">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/upsc_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/upsc"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/upsc"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/upsc_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/upsc"
                                android:text="UPSC"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cat_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/cat"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/cat"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/cat_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/cat"
                                android:text="CAT"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll3"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@id/ll2">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/neet_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/neet"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/neet"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/neet_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/neet"
                                android:text="Neet &amp; AIIMS"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/gate_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/gate"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/gate"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/gate_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/gate"
                                android:text="GATE"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll4"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_marginBottom="10dp"
                    android:layout_below="@id/ll3">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cds_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/ielts"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/ielts"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/cds_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/ielts"
                                android:text="CDS"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cgs_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/toelf"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/toelf"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/toelf_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/toelf"
                                android:text="Combined Geo-Scientist (Main)"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/ll5"
                    android:layout_width="match_parent"
                    android:layout_height="200dp"
                    android:weightSum="20"
                    android:layout_below="@id/ll4">
                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/nda_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginRight="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/cmt_nda"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/cmt_nda"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/cmt_nda_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/cmt_nda"
                                android:text="NDA"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/ies_category"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="10"
                        android:theme="@style/Theme.MaterialComponents.Light"
                        app:cardElevation="8dp"
                        app:cardCornerRadius="8dp"
                        android:layout_marginLeft="5dp"
                        android:clickable="true"
                        android:focusable="true"
                        android:checkable="true">
                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:orientation="vertical"
                            android:background="@color/colorPrimaryDark">
                            <ImageView
                                android:id="@+id/other"
                                android:layout_width="match_parent"
                                android:layout_height="150dp"
                                android:src="@drawable/other"
                                android:scaleType="fitXY"/>
                            <TextView
                                android:id="@+id/other_text"
                                android:layout_width="match_parent"
                                android:layout_height="50dp"
                                android:layout_below="@+id/other"
                                android:text="Indian Economic Service"
                                android:textColor="@color/white"
                                android:autoSizeMinTextSize="15dp"
                                android:autoSizeMaxTextSize="20dp"
                                android:autoSizeTextType="uniform"
                                android:gravity="center"
                                android:fontFamily="@font/pt_sans_narrow_regular"
                                />
                        </RelativeLayout>
                    </com.google.android.material.card.MaterialCardView>
                </LinearLayout>
            </RelativeLayout>
        </ScrollView>
        </RelativeLayout>
        <com.google.android.material.navigation.NavigationView
            android:id="@+id/navigation_view"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:clipToPadding="false"
            app:headerLayout="@layout/side_navigation_header"
            app:menu="@menu/menu" />
    </androidx.drawerlayout.widget.DrawerLayout>
</LinearLayout>